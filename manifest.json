{
  "manifest_version": 3,
  "name": "Gitlab Approved",
  "version": "0.1",
  "description": "Chrome extension to show how many approves an MR has",
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["*://*/"],
  "icons": {
    "16": "gitlab-16x16.png",
    "48": "gitlab-48x48.png",
    "128": "gitlab-128x128.png"
  }
}